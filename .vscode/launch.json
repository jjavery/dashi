  "version": "0.2.0",
  "configurations": [
    {
      "name": "Encrypt",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/dashi/",
      "cwd": "${workspaceFolder}",
      "args": [
        "--encrypt",
        "-i=test.secret",
        "-r=XUESSUZJTCPJT7THPA4TSRM6EQ32HVY57COGWVZCRVXXNSMGMCNA",
        "-o=test.enc",
        "test.txt"
      ]
    },
    {
      "name": "Decrypt",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/dashi/",
      "cwd": "${workspaceFolder}",
      "args": [
        "--decrypt",
        "-i=test.secret",
        "-o=-",
        "test.enc"
      ]
    }
  ]
}
